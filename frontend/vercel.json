{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "trailingSlash": true,
  "cleanUrls": false,
  "env": {
    "NEXT_PUBLIC_WS_URL": "wss://president-engine.koyeb.app/ws"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 